<?xml version="1.0" encoding="UTF-8"?>
<completions>

  <provider name="kirby.icons">
    <syntax>yaml</syntax>
    <selector>*:not(comment)</selector>
    <!-- After space: -->
    <expression>(?&lt;=icon\:\s)([a-zA-Z]*)</expression>
    <set>kirby.icons</set>
  </provider>

  <provider name="kirby.helpers">
    <syntax>php-base</syntax>
    <selector>*:not(comment)</selector>
    <!-- After space -->
    <expression>(?&lt;=\s)([a-zA-Z]*)</expression>
    <set>kirby.helpers</set>
  </provider>
  
  <provider name="kirby.objects">
    <syntax>php-base</syntax>
    <selector>*:not(comment)</selector>
    <!-- After $ -->
    <expression>(?&lt;=\$)([a-zA-Z]*)</expression>
    <set>kirby.objects</set>
  </provider>

  <provider name="kirby.methods">
    <syntax>php-base</syntax>
    <selector>*:not(comment)</selector>
    <!-- After > -->
    <expression>(?&lt;=(\-\>))([a-zA-Z]*)</expression>
    <set>kirby.methods</set>
  </provider>
  
